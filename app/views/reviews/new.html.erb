<%= form_for [@book, @review] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :rating %>
  <%= f.number_field :rating %>

  <%= f.label :review_text %>
  <%= f.text_field :review_text %>

  <%= f.submit %>
 <% end %>
